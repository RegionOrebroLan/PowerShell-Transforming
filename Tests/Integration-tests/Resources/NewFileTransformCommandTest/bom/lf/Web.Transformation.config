<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<appSettings>
		<add key="Transformed-Key" value="From Web.Transformation.config" xdt:Locator="Match(key)" xdt:Transform="SetAttributes(value)" />
	</appSettings>
	<connectionStrings>
		<add name="Transformed-Connection-String" connectionString="From Web.Transformation.config" xdt:Locator="Match(name)" xdt:Transform="SetAttributes(connectionString)" />
	</connectionStrings>
</configuration>